<nav class="navbar navbar-default" ng-controller="searchCtr">
  <div class="container">
   <div class="row">
     <div class="col-md-12">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle pull-right top-search" ng-click="searchbox_hide()">

         <span class="glyphicon" ng-class="myglyphicon"></span>    

         
       </button>

       <a href="#" ng-hide="logo==1" class="navbar-toggle pull-right top-flag"  data-toggle="modal" data-target="#modalLanguage">

        {{#is lang "en"}}  <img height="32" src="bower_components/flag-icon-css/flags/4x3/us.svg">{{/is}}
        {{#is lang "vi"}}<img height="32" src="bower_components/flag-icon-css/flags/4x3/vn.svg">{{/is}}
        {{#is lang "ko"}} <img height="32" src="bower_components/flag-icon-css/flags/4x3/kr.svg">{{/is}}
        {{#is lang "zh"}} <img height="32" src="bower_components/flag-icon-css/flags/4x3/cn.svg">{{/is}}
      </a>
      <a ng-hide="logo==1" class="navbar-brand" href="/"><img src="/images/logo_videodown.png" alt="logo"></a>
    </div>
    <div ng-class="{showhide: !logo}" id="myNavbar">
      <ul class="nav navbar-nav navbar-right">
        <li>
        </li>
        <li class="hidden-xs li-language"><a href="#" data-toggle="modal" data-target="#modalLanguage">Language</a></li>

        <form id="form_search" action="/search/make_url" name="search" class="navbar-form navbar-right ng-pristine ng-valid" role="form" method="GET" accept-charset="utf-8">
          <div class="input-group">
            <input type="text" name="search_text" class="form-control" id="youtube" required placeholder="Search">
            <div class="input-group-btn">
              <button class="btn btn-success" type="submit"><i class="glyphicon glyphicon-search"></i></button>
            </div>
          </div>
        </form>
        {{#if user }}
        <li class="dropdown">
          <a class="dropdown-toggle" data-toggle="dropdown" href="#"><span class="glyphicon glyphicon-user"></span> <strong>{{user.name}}</strong>
            <ul class="dropdown-menu">
              <li><a href="/dashboard">Daskboard</a></li>
              <li><a href="/user/logout"><span class="glyphicon glyphicon-log-in"></span> Logout</a></li>
            </ul>
          </li>
          {{/if}}
        </ul>
      </div>
    </div>
  </div>
</div>
</nav>
<div class="wrapper"  ng-cloak>
  {{clang}}
  {{{body}}}

  <div class="container">
   <div class="row">
     <div class="col-md-12">
       <h1>{{trans.resent_search}}</h1>
       <div class="current-search">
         {{#each kwords.arr}}
         <a {{#is @index '>' 20 }} class="hidden-sm hidden-xs" {{/is}} href="/keyword/{{this.slug}}"> 
           <span class="alert alert-info">{{this.name}}</span></a>
           {{/each}}
         </div>
         <br>
         <br>
       </div>
     </div>
   </div>

   <!--for sticky footer-->
