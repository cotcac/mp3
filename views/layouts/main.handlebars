<nav class="navbar navbar-default" ng-controller="searchCtr">
  <div class="container">
   <div class="row">
     <div class="col-md-12">
      <div class="navbar-header">
       <button type="button" class="navbar-toggle" ng-click="searchbox_hide()">
        <span class="glyphicon" ng-class="myglyphicon"></span>    
      </button>
      <a ng-hide="logo==1" class="navbar-brand" href="/"><img src="/images/videodown.png" alt="logo"></a>
    </div>
    <div ng-class="{showhide: !logo}" id="myNavbar">

      <ul class="nav navbar-nav navbar-right">

       <form id="form_search" action="/search/make_url" name="search" class="navbar-form navbar-right ng-pristine ng-valid" role="form" method="GET" accept-charset="utf-8">
        <div class="input-group">
          <input type="text" name="search_text" class="form-control" id="youtube" required placeholder="Search">
          <div class="input-group-btn">
            <button class="btn btn-success" type="submit"><i class="glyphicon glyphicon-search"></i></button>
          </div>
        </div>
      </form>
      {{#if user }}
      <li class="dropdown">
        <a class="dropdown-toggle" data-toggle="dropdown" href="#"><span class="glyphicon glyphicon-user"></span> <strong>{{user.name}}</strong>
          <ul class="dropdown-menu">
            <li><a href="/dashboard">Daskboard</a></li>
            <li><a href="/user/logout"><span class="glyphicon glyphicon-log-in"></span> Logout</a></li>
          </ul>
        </li>
        {{/if}}
      </ul>
    </div>
  </div>
</div>
</div>
</nav>
<div class="wrapper"  ng-cloak>
  {{clang}}
  {{{body}}}

  <div class="container">
   <div class="row">
     <div class="col-md-12">
       <h1>{{trans.resent_search}}</h1>
       <div class="current-search">
         {{#each kwords.arr}}
         <a {{#if knowds.haimuoi}} class="hidden-sm hidden-xs"{{/if}} href="/keyword/{{this.slug}}"> 
           <span class="alert alert-info">{{this.name}}</span></a>
           {{/each}}
         </div>
         <br>
         <br>
       </div>
     </div>
   </div>

   <!--for sticky footer-->
   <div class="push"></div>
 </div><!-- end  wrapper -->
 <footer class="container-fluid footer">
  <div class="container">
    <div class="row text-center">  
      <div class="col-md-3 col-xs-12">
        <a class="navbar-brand" href="/"><img class="img-responsive b_logo" src="/images/videodown.png" alt="VideoDown logo footer"></a>
      </div>
      <div class="col-md-7 col-xs-12">
        <br>
        <p><span class="glyphicon glyphicon-copyright-mark"></span> 2017 <a href="/">Videodown.cc</a> All Rights Reserved
         
        </p>
      </div>
      <div class="col-md-2 col-xs-12">


        <a {{#is lang "en"}} class="active_lang" {{/is}} href="/?clang=en">En</a>
        <a {{#is lang "vi"}} class="active_lang" {{/is}} href="/?clang=vi">Vi</a>
        <a {{#is lang "ko"}} class="active_lang" {{/is}}  href="/?clang=ko">Ko</a>
        <a {{#is lang "zh"}} class="active_lang" {{/is}}  href="/?clang=zh">Zh</a>
      </div>
    </div>
  </div>
  <br>
</footer>

